		"react-redux": "^7.1.3",
		"redux": "^4.0.5"
	},
	"devDependencies": {
		"@types/expo": "^32.0.13",
		"@types/jest": "^24.0.18",
		"@types/react": "^16.8.23",
		"@types/react-dom": "^16.9.0",
		"@types/react-native": "^0.57.65",
